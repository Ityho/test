<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.miduchina.wrd.api.mapper.heatanalysis.HeatMapper">
    <select id="findShareCode" resultType="com.miduchina.wrd.po.home.HeatShare" >
        <![CDATA[
            SELECT
            *
            FROM
            heat_share
            WHERE
            share_code=#{shareCode}
        ]]>

    </select>



</mapper>