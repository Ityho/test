<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.miduchina.wrd.api.mapper.user.UserChannelRelationMapper">

    <insert id="insertChannelRelation" useGeneratedKeys="true" keyProperty="id">
        <![CDATA[
           INSERT INTO
				user(
					user_id,
					uec_id,
					status,
					create_time,
					update_time
				)
			VALUES
				(
				#{userId},
				#{uecId},
                #{status},
				now(),
				now());
        ]]>
    </insert>


</mapper>