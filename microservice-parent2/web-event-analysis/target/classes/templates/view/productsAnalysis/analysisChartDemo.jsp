	function selectTime(n,obj){
		$(obj).addClass("selected");
		$(obj).siblings('li').removeClass('selected');
		var curDate = new Date(); 
		var preDate = new Date(curDate.getTime() - parseInt(n)*24*60*60*1000);  //前n天
