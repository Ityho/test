<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.miduchina.wrd.api.research.mapper.ApplicantMapper">

    <insert id="doSaveApplicant" useGeneratedKeys="true" keyProperty="id">
        <![CDATA[
           INSERT INTO
				applicant_info(
                    type,
                    name,
                    applicant,
                    unit,
                    data,
                    email,
                    tel,
                    address
				)
			VALUES
				(
				#{type},
				#{name},
				#{applicant},
				#{unit},
				now() ,
				#{email},
				#{tel},
				#{address}
				)
        ]]>
    </insert>

</mapper>