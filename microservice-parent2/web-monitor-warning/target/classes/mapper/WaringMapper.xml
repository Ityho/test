<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.miduchina.wrd.monitor.warning.mapper.WarningMapper">

    <select id="findById" resultType="com.miduchina.wrd.po.keyword.Warning">
        SELECT
          *
        FROM
          `warning`
        where status = 1 and warning_id = #{id}
    </select>

</mapper>